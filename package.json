{
  "name": "git-eslint",
  "version": "0.1.0",
  "description": "Filter ESLint output by git history",
  "main": "build/cli.js",
  "scripts": {
    "build": "babel src -d build",
    "flow": "flow",
    "lint": "eslint src/*.js",
    "prepare": "npm run build"
  },
  "keywords": [
    "lint",
    "eslint",
    "git"
  ],
  "author": "Sylvan Mably",
  "license": "MIT",
  "dependencies": {
    "eslint": "^4.18.1",
    "minimatch": "^3.0.4",
    "nodegit": "^0.20.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "flow-bin": "^0.66.0",
    "prettier": "^1.10.2"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 4
          }
        }
      ],
      "flow"
    ]
  }
}
