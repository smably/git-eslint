{
  "name": "git-eslint",
  "version": "0.1.0",
  "description": "Filter ESLint output by git history",
  "main": "build/cli.js",
  "scripts": {
    "build": "babel src -d build",
    "lint": "eslint src/*.js"
  },
  "keywords": [
    "lint",
    "eslint",
    "git"
  ],
  "author": "Sylvan Mably",
  "license": "MIT",
  "dependencies": {
    "eslint": "^3.19.0",
    "minimatch": "^3.0.3",
    "nodegit": "^0.19.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.4.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-config-prettier": "^1.7.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-prettier": "^2.0.1",
    "prettier": "^1.2.2"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 4
          }
        }
      ]
    ]
  }
}
